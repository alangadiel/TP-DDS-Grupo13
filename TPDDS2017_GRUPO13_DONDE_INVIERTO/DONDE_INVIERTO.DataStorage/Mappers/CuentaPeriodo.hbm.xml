<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DONDE_INVIERTO.Model"
                   namespace="DONDE_INVIERTO.Model">
  <class name="CuentaPeriodo"  table="cuenta_periodo">
    <id name="Id"
        column="cupe_id">
      <generator class="native" />
    </id>
    <bag name="Cuenta">
      <key column="cupe_cuenta_id" />
      <one-to-many class="Cuenta" />
    </bag>
    <bag name="Periodo">
      <key column="cupe_periodo_id" />
      <one-to-many class="Periodo" />
    </bag>
  </class>
</hibernate-mapping>