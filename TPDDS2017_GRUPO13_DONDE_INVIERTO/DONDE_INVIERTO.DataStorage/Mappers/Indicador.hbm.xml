<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DONDE_INVIERTO.Model"
                   namespace="DONDE_INVIERTO.Model">
  <class name="Indicador"  table="indicador">
    <id name="Id"
        column="ind_id">
      <generator class="native" />
    </id>
    <property name="Nombre" column="ind_nombre" type="string" length="200" not-null="true"/>
    <property name="Contenido" column="ind_contenido" type="string" length="200" not-null="true"/>
  </class>
</hibernate-mapping>