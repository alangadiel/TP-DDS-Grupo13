<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DONDE_INVIERTO.Model"
                   namespace="DONDE_INVIERTO.Model">
  <class name="Usuario"  table="usuario">
    <id name="Id"
        column="usu_id">
      <generator class="native" />
    </id>
    <property name="Nombre" column="usu_nombre" type="string" length="200" not-null="true"/>
    <property name="Contrasenia" column="usu_contrasenia" type="string" length="200" not-null="true"/>
  </class>
</hibernate-mapping>