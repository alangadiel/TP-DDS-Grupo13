<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DONDE_INVIERTO.Model" assembly="DONDE_INVIERTO.Model">
	<class name="Empresa" table="empresas" lazy="false">
		<id name="Id" column="empr_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="FechaFundacion" column="empr_fecha_fundacion" not-null="false" />
		<property name="Nombre" column="empr_nombre" not-null="false" length="200" />
    <property name="Cuit" column="empr_cuit" not-null="false" length="11" />
	</class>
</hibernate-mapping>
