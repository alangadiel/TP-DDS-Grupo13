<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DONDE_INVIERTO.Model" assembly="DONDE_INVIERTO.Model">
	<class name="Usuario" table="usuarios" lazy="false">
		<id name="Id" column="usu_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Contrasenia" column="usu_contrasenia" not-null="true" length="20" />
		<property name="Username" column="usu_usuario" not-null="true" length="20" />
	</class>
</hibernate-mapping>
