<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DONDE_INVIERTO.Model" assembly="DONDE_INVIERTO.DataStorage">
	<class name="Balance" table="balances" lazy="false">
		<id name="Id" column="bala_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="EmpresaId" column="bala_empresa_id" not-null="true" />
		<property name="Periodo" column="bala_periodo" not-null="true" />
		<property name="Valor" column="bala_valor" not-null="true" />
	</class>
</hibernate-mapping>
