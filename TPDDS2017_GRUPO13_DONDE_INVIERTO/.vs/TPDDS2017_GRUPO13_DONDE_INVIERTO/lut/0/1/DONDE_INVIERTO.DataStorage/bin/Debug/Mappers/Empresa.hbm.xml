<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DONDE_INVIERTO.Model"
                   namespace="DONDE_INVIERTO.Model">
  <class name="Empresa"  table="Empresa">
    <id name="Id"
        column="emp_id">
      <generator class="native" />
    </id>
    <property name="Nombre" column="emp_nombre" type="string" length="200" not-null="true"/>
    <property name="Fecha_Creacion" type="datetime" column="emp_fecha_creacion" not-null="true"/>
  </class>
</hibernate-mapping>