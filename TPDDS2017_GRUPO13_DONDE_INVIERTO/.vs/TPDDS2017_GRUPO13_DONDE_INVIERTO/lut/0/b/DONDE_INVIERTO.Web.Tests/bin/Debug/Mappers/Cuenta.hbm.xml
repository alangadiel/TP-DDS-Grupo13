<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DONDE_INVIERTO.Model"
                   namespace="DONDE_INVIERTO.Model">
  <class name="Cuenta"  table="cuenta">
    <id name="Id"
        column="cue_id">
